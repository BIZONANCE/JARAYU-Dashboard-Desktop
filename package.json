{
  "name": "admin-panel-jarayu",
  "version": "0.0.3",
  "description": "JARAYU Ayurved Admin Panel",
  "main": "main.js",
  "author": "Bizonance",
  "license": "ICSC",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "release": "electron-builder --publish always"
  },
  "devDependencies": {
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8"
  },
  "dependencies": {
    "electron-log": "^5.2.4",
    "electron-updater": "^6.3.9"
  },
  "build": {
    "appId": "com.bizonance.adminpanel.jarayu",
    "productName": "JARAYUAYURVED-Dashboard",
    "copyright": "Copyright Â© Bizonance",
    "artifactName": "JARAYUAYURVED-Dashboard-Setup-${version}.${ext}",

    "win": {
      "target": [
        "nsis"
      ],
      "icon": "./bizonance_logo.png",
      "publisherName": "Bizonance",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "mac": {
      "target": "dmg"
    },
    "directories": {
      "output": "dist"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "BIZONANCE",
        "repo": "JARAYU-Dashboard-Desktop",
        "releaseType": "release"
      }
    ]
  }
}